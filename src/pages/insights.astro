---
Astro.response.headers.set(
	'CDN-Cache-Control',
	'public, max-age=604800, must-revalidate',
)

export const prerender = false
export const pagefind = false

import SSRPage from '@layouts/SSRPage.astro'
import _getInsights from '@scripts/all-insights'
import { _addInsights } from '@scripts/store'

const insights = await _getInsights()

_addInsights(insights)
---

<SSRPage />
