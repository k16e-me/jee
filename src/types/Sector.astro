---
import HeaderExpertise from '@composites/headers/HeaderExpertise.astro'
import Grid from '@layouts/Grid.astro'
import StoryblokComponent from '@storyblok/astro/StoryblokComponent.astro'

const { blok } = Astro.props
---

<HeaderExpertise
	item={blok}
	heads={blok.chair[0]}
	breadcrumbLink='/sectors'
	breadcrumbLabel='JEE Sectors'
	class='col-span-full'
/>
<div class='col-span-full'>
	{
		blok.body.map((blok) => {
			return <StoryblokComponent blok={blok} />
		})
	}
</div>
